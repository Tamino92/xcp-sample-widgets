<?xml version="1.0" encoding="UTF-8"?>
<function xmlns="http://documentum.emc.com/2010/UI-Model"
          id="JSFunction-ratingsFA" version="1.0.0000.001">

    <name>ratingsFA</name>
    <description>Show value with star rating relative to max value</description>
    <content src="content/dsvg/functions/ratingsFontAwesome.js" type="text/javascript"/>
    <content src="resources/css/dsvg/ratings.css" type="text/css"/>


    <namespace>dsvg.functions</namespace>
    <categoryName>String</categoryName>

    <returnType isRepeating="false">STRING</returnType>
    <params>
        <param name="max" required="true">
            <type isRepeating="false">INTEGER</type>
        </param>
        <param name="value" required="true">
            <type isRepeating="false">INTEGER</type>
        </param>
        <param name="color" required="false">
            <type isRepeating="false">STRING</type>
        </param>
    </params>
</function>